package Methods;

/**
 *
 * @author Nicolas
 */
public class Population {
    private int _intWidth;
    private String _strIncreasingStyle = "";
    
    /* BEGIN GETTERS AND SETTERS */
    public int getWidth(){
        return this._intWidth;
    }
    
    public void setWidth(int intWidth){
        this._intWidth = intWidth;
    }
    
    public String getIncreasingStyle(){
        return this._strIncreasingStyle;
    }
    
    public void setIncreasingStyle(String strIncreasingStyle){
        this._strIncreasingStyle = strIncreasingStyle;
    }
    /* END GETTERS AND SETTERS */
    
    public Population(int intWidth, String strIncreasingStyle){
        this._intWidth = intWidth;
        this._strIncreasingStyle = strIncreasingStyle;
    }
    
}
