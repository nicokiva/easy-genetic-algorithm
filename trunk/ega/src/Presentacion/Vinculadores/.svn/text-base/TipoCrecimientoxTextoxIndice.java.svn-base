package Presentacion.Vinculadores;

import enums.TiposCrecimiento;
import java.util.HashMap;

public class TipoCrecimientoxTextoxIndice extends HashMap{
    private TiposCrecimiento TC;
    private Integer Indice;
    private String Texto = new String();
    
    public TipoCrecimientoxTextoxIndice(TiposCrecimiento TC, Integer Indice, String Texto){
        this.TC = TC;
        this.Indice = Indice;
        this.Texto = Texto;
    }
    
    public TiposCrecimiento ObtenerTipoCrecimiento(){
        return this.TC;
    }
    
    public Integer ObtenerIndice(){
        return this.Indice;
    }
    
    public String ObtenerTexto(){
        return this.Texto;
    }
}
